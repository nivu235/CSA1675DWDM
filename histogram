names = c("Frank", "John", "Kesav","priya","aravind","roja")
age<-c(23,21,22,25,58,5)
tumorsize<-c(88,66,79,50,70,80)
invnodes<-c(20,30,40,60,50,80)
df<-data.frame(names,age,tumorsize,invnodes)
View(df)
write.csv(df,"breastcancer.csv")
data<-read.csv("breastcancer.csv")
View(data)
hist(data$age,xlab = "age",ylab = "no of person at this age",col="red")
